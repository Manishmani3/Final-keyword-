public class Finalname{
   void changename(String name){
   name="dinesh";
   System.out.println(name);
   }

 public static void main(String [] args){
   Finalname ganesh=new Finalname();
   final String name="ganesh";
   ganesh.changename(name);
   //name="haja";
   }
}

output as

dinesh

